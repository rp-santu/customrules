{
    "name": "rpqa99/rp-library",
    "description": "Simple useful library",
    "type": "craft-plugin",
    "version": "1.0.1",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "rp library"
    ],
    "support": {
        "docs": "https://github.com/rpqa99/rp-library/blob/master/README.md",
        "issues": "https://github.com/rpqa99/rp-library/blob/master/CHANGELOG.md"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Bedh Prakash",
            "homepage": "https://github.com/bedh-rp"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0"
    },
    "autoload": {
        "psr-4": {
          "rpqa99\\rplibrary\\": "src/"
        }
    },
    "extra": {
        "name": "rp library",
        "handle": "rp-library",
        "developer": "Bedh Prakash",
        "developerUrl": "https://github.com/bedh-rp",
        "documentationUrl": "https://github.com/rpqa99/rp-library/blob/master/README.md",
        "changelogUrl": "https://github.com/rpqa99/rp-library/blob/master/CHANGELOG.md",
        "class": "rpqa99\\rplibrary\\RpLibrary"
    }
}
